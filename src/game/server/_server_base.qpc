// ---------------------------------------------------------------
// Base Server Game Module Script
// ---------------------------------------------------------------
macro OUTBINNAME "server"
macro OUTBINDIR "$SRCDIR/../game/$GAMENAME/bin/$PLATFORM"
macro GAMENAME

include "$SRCDIR/_qpc_scripts/source_dll_base.qpc"

configuration
{
	general
	{
		include_directories
		{
			"$SRCDIR/game/server"
			"$SRCDIR/game/shared"
			"$SRCDIR/utils/common"
		}
	}

	compiler
	{
		preprocessor_definitions
		{
			"GAME_DLL"
			"VECTOR"
			"PROTECTED_THINGS_ENABLE"
			"fopen=dont_use_fopen"
			"sprintf=use_Q_snprintf_instead_of_sprintf"
			"strncpy=use_Q_strncpy_instead"
			"_snprintf=use_Q_snprintf_instead"
		}

		precompiled_header "use"
		precompiled_header_file "cbase.h"
	}

	linker
	{
		libraries
		{
			"winmm.lib" [$WINDOWS]
			"choreoobjects"
			"dmxloader"
			"mathlib"
			"particles"
			"tier2"
			"tier3"
			"steam_api"
		}
	}
}

dependencies
{
	"choreoobjects/choreoobjects.qpc"
	"dmxloader/dmxloader.qpc"
	"mathlib/mathlib.qpc"
	"particles/particles.qpc"
	"tier2/tier2.qpc"
	"tier3/tier3.qpc"
}

